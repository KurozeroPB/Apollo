{
    "name": "apollo",
    "private": true,
    "version": "3.0.0-dev3",
    "description": "Private API Service",
    "license": "MIT",
    "engines": {
        "node": ">=12",
        "npm": ">=5.6.0",
        "yarn": ">=1.22.0 <2.0.0"
    },
    "homepage": "https://github.com/KurozeroPB/Apollo#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/KurozeroPB/Apollo.git"
    },
    "bugs": {
        "url": "https://github.com/KurozeroPB/Apollo/issues"
    },
    "author": {
        "email": "root@kurozeropb.info",
        "name": "Pepijn van den Broek",
        "url": "https://github.com/KurozeroPB"
    },
    "scripts": {
        "deploy": "npm run pm2:prod",
        "pm2:prod": "pm2 start ecosystem.config.js --env production",
        "pm2:dev": "pm2 start ecosystem.config.js --env development",
        "prebuild": "rm -rf dist",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/server.ts",
        "test": "npm run dev",
        "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/server.ts",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
    },
    "dependencies": {
        "@kurozero/collection": "^0.5.0",
        "@sentry/node": "^5.19.1",
        "axios": "^0.19.2",
        "body-parser": "^1.19.0",
        "canvas": "^2.6.1",
        "canvas-multiline-text": "^1.0.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "eris": "^0.13.3",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.3",
        "express-robots-txt": "^0.4.1",
        "express-user-agent-blocker": "^2.0.3",
        "git-rev-sync": "^2.0.0",
        "graphql": "^15.3.0",
        "graphql-import-node": "0.0.4",
        "helmet": "^3.23.3",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.27.0",
        "mongoose": "^5.9.22",
        "morgan": "^1.10.0",
        "reflect-metadata": "^0.1.13",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.7",
        "@types/express-rate-limit": "^5.0.0",
        "@types/git-rev-sync": "^2.0.0",
        "@types/helmet": "0.0.47",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mongoose": "^5.7.30",
        "@types/morgan": "^1.9.1",
        "@types/node": "^14.0.20",
        "@typescript-eslint/eslint-plugin": "^3.6.0",
        "@typescript-eslint/parser": "^3.6.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.4.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "gts": "^2.0.2",
        "nodemon": "^2.0.4",
        "npm-reinstall": "^3.1.0",
        "prettier": "^2.0.5",
        "ts-loader": "^8.0.0",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.6"
    }
}
